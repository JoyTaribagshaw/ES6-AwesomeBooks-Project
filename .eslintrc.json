{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-useless-escape": "off",
    "class-methods-use-this": "off",
    "no-continue": "off",
    "guard-for-in": "off",
    "brace-style": "off",
    "no-unused-vars": "off",
    "max-classes-per-file": "off",
    "no-underscore-dangle": "off",
    "no-loop-func": "off",
    "no-plusplus": "off",
    "no-restricted-globals": "off",
    "no-use-before-define": "off",
    "prefer-const": "off",
    "no-restricted-syntax": "off",
    "default-case": "off",
    "prefer-rest-params": "off",
"no-nested-ternary": "off",
    "max-len": "off",
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "import/extensions": [ 1, {
      "js": "always", "json": "always"
    }]
  },
  "ignorePatterns": [
    "dist/",
    "build/"
  ]
}